{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch qt-material-widgets (Windows)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/examples/RelWithDebInfo/examples.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/examples/RelWithDebInfo",
            "environment": [
                {
                    "name": "PATH",
                    "value": "${env:PATH};${env:CMAKE_PREFIX_PATH}/bin"
                }
            ],
            "preLaunchTask": "cmake-build-dev",
            "externalConsole": false
        },
        {
            "name": "Launch qt-material-widgets (Windows Debug)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/examples/Debug/examples.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/examples/Debug",
            "environment": [
                {
                    "name": "PATH",
                    "value": "${env:PATH};${env:CMAKE_PREFIX_PATH}/bin"
                }
            ],
            "preLaunchTask": "cmake-build-debug",
            "externalConsole": false
        },
        {
            "name": "Launch qt-material-widgets (Linux/macOS)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/examples/examples",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/examples",
            "environment": [
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "${env:CMAKE_PREFIX_PATH}/lib:${workspaceFolder}/build/components"
                }
            ],
            "preLaunchTask": "cmake-build-dev-ninja",
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}